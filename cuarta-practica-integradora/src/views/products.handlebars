<main>

  <h1>Bienvenido</h1>
    <div class="row justify-content: space-evenly">
  {{#each products}}
  <div class="d-flex col col-xxl-6 col-xl-3 col-lg-6" style="width: 18rem; height:520px">
      <div class="card" style="width: 18rem; height:520px">
        <p>thumbnails:</p>
        <img class="card-img-top" style="width: 287px; height: 150px" src="{{this.thumbnails}}" alt="">
        <div class="card-body">
          <h5 class="card-title">title: {{this.title}}</h5>
          <p class="card-text">description: {{this.description}}</p>
          <h5 class="card-title">code: {{this.code}}</h5>
          <h5 class="card-title">price: {{this.price}}</h5>
          <h5 class="card-title">status: {{this.status}}</h5>
          <h5 class="card-title">stock: {{this.stock}} </h5>
          <h5 class="card-title">category: {{this.category}}</h5>
          <a href="/products/{{this._id}}" class="btn btn-primary">Detalle</a>
          <button class="btn btn-success" onclick="putIntoCart('{{this._id}}')">Agregar al Carrito</button>
        </div>
      </div>
    </div>
  {{/each}}
  </div>
  {{#if paginate.hasPrevPage}}
  <a href="/products?page= {{paginate.prevPage}}">{{paginate.prevPage}}</a>
  {{/if}}
  <a> {{paginate.page}} </a>
  {{#if paginate.hasNextPage}}
  <a href="/products?page= {{paginate.nextPage}}">{{paginate.nextPage}}</a>
  {{/if}}
  </br>

  {
  status: "succes"
  totalDocs:{{paginate.totalDocs}}
  limit:{{paginate.limit}}
  totalPages:{{paginate.totalPages}}
  page:
  {{paginate.page}}
  pagingCounter:{{paginate.pagingCounter}}
  hasPrevPage:{{paginate.hasPrevPage}}
  hasNextPage:{{paginate.hasNextPage}}
  prevPage:{{paginate.prevPage}}
  nextPage:{{paginate.nextPage}}
  }

  <script src="/js/product.js"></script>
</main>